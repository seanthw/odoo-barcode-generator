<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Barcodes -->
        <record id="sequence_barcode" model="ir.sequence">
            <field name="name">Barcode Sequence</field>
            <field name="code">barcode.sequence</field>
            <field name="prefix">%(y)s%(month)s%(day)s</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
